  "babel": {
    "presets": [
      "react",
      "node5",
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "__DEV__": true,
      "browser": true,
      "expect": true
    },
    "env": {
      "browser": true,
      "mocha": true
    },
    "rules": {
      "no-underscore-dangle": 0,
      "global-require": 0,
      "no-confusing-arrow": 0
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "string-quotes": "single",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global",
            "local"
          ]
        }
      ]
    },
    "ignoreFiles": [
      "./src/public/**"
    ]
  },
  "scripts": {
    "eslint": "eslint src tools test",
    "lintfix": "eslint src tools test --fix",
    "stylelint": "stylelint \"src/**/*.css\"",
    "lint": "npm run eslint && npm run stylelint",
    "test": "npm run lint && rimraf coverage && npm run test:coverage && istanbul check-coverage",
    "test:watch": "mocha src/**/*.test.js test --require test/setup.js --compilers js:babel-register --reporter min --watch",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "deploy": "babel-node tools/run deploy",
    "test:unit": "env PORT=4000 MONGOLAB_URI=mongodb://localhost/onetest mocha 'src/**/*.test.js' 'src/**/*test.js' test ",
    "test:coverage": "env PORT=4000 MONGOLAB_URI=mongodb://localhost/onetest istanbul cover _mocha 'src/**/*.test.js' 'src/**/*test.js' test ",
    "screenshots:local": "pageres --delay=2 http://localhost:3000 1024x768 1280x1024 1024x768 1920x1080 600x480 2560x1440 1600x1200 ",
    "psi": "gulp psi",
    "test:integration-local": "wdio integration-tests/wdio.conf.js",
    "test:integration-local-bs": "wdio integration-tests/wdio.conf.bs.local.js",
    "test:integration-heroku": "wdio integration-tests/wdio.conf.staging.js",
    "test:integration-heroku-bs": "wdio integration-tests/wdio.conf.staging.bs.js",
    "render": "babel-node tools/run render",
    "start": "babel-node tools/run start"
  },
  "version": "0.9.5"
}
